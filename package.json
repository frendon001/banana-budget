{
  "name": "banana-budget",
  "version": "1.0.0",
  "private": true,
  "description": "Solving for banana budget problem.",
  "main": "index.js",
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/yup": "^0.26.37",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "concurrently": "^5.2.0",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "moment": "^2.29.4",
    "nodemon": "^1.19.4",
    "path-parser": "^4.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "yup": "^0.28.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^5.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^7.1.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^2.3.6",
    "ts-loader": "^6.2.2",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "start": "node build/index.js",
    "server": "npm run validate && nodemon build/index.js",
    "test": "mocha --exit",
    "client": "webpack-dev-server --open --config webpack.dev.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=true npm run build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|jsx|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel check-types check-format lint build:server",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "babel server --extensions .js,.ts,.tsx --out-dir build",
    "build": "npm run build:server && npm run build:client"
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "eslint"
    ],
    "**/*.+(js|json|ts|tsx)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged && npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/frendon001/banana-budget.git"
  },
  "author": "",
  "license": "ISC"
}
