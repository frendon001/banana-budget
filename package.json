{
  "name": "banana-budget",
  "version": "1.0.0",
  "private": true,
  "description": "Solving for banana budget problem.",
  "main": "index.js",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/yup": "^0.26.32",
    "axios": "^0.19.1",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "formik": "^2.1.2",
    "moment": "^2.24.0",
    "nodemon": "^1.18.10",
    "path-parser": "^4.2.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.7.7",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "babel-loader": "^8.0.6",
    "chai": "^4.2.0",
    "chai-http": "^4.2.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "scripts": {
    "start": "node build/index.js",
    "server": "npm run validate && nodemon build/index.js",
    "test": "mocha --exit",
    "client": "webpack-dev-server --open --config webpack.dev.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=true npm run build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx,.jsx .",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|json|jsx|ts|tsx)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "check-types": "tsc",
    "validate": "npm-run-all --parallel check-types check-format lint build:server",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "babel server --extensions .js,.ts,.tsx --out-dir build",
    "build": "npm run build:server && npm run build:client"
  },
  "lint-staged": {
    "*.+(js|ts|tsx)": [
      "eslint"
    ],
    "**/*.+(js|json|ts|tsx)": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check-types && lint-staged && npm run build"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/frendon001/banana-budget.git"
  },
  "author": "",
  "license": "ISC"
}
